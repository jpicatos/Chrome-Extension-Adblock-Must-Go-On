{
    "manifest_version": 2,
    "name": "Disable Anti-Adblock Popup",
    "version": "1.0.1",
    "description": "Disable adblock popup",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "browser_action": {
        "default_title": "Disable Anti-Adblock Popup",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["in-content.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; img-src * data: 'self' 'unsafe-eval'",
    "offline_enabled": true,
    "permissions": ["contextMenus"]
}
